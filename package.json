{
  "name": "node-js-jwt-auth",
  "version": "1.0.0",
  "description": "Node.js Demo for JWT Authentication",
  "main": "server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node server.js",
    "ssh": "ssh root@194.67.90.19",
    "server": "node server.js",
    "client": "npm run start --prefix client",
